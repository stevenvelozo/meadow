{
  "name": "meadow",
  "version": "1.1.1",
  "description": "A data access library.",
  "main": "source/Meadow.js",
  "scripts": {
    "start": "node source/Meadow.js",
    "coverage": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -u tdd -R spec",
    "test": "./node_modules/mocha/bin/_mocha -u tdd -R spec",
    "tests": "./node_modules/mocha/bin/_mocha -u tdd -R spec --grep",
    "docker-dev-build-image": "docker build ./ -t retold/meadow:local",
    "docker-dev-run": "docker run -it -d --name meadow-dev -p 127.0.0.1:12342:8080 -p 12302:3306 -v \"$PWD/.config:/home/coder/.config\"  -v \"$PWD:/home/coder/meadow\" -u \"$(id -u):$(id -g)\" -e \"DOCKER_USER=$USER\" retold/meadow:local"
  },
  "mocha": {
    "diff": true,
    "extension": [
      "js"
    ],
    "package": "./package.json",
    "reporter": "spec",
    "slow": "75",
    "timeout": "5000",
    "ui": "tdd",
    "watch-files": [
      "source/**/*.js",
      "test/**/*.js"
    ],
    "watch-ignore": [
      "lib/vendor"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenvelozo/meadow.git"
  },
  "keywords": [
    "orm",
    "dal"
  ],
  "author": "Steven Velozo <steven@velozo.com> (http://velozo.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevenvelozo/meadow/issues"
  },
  "homepage": "https://github.com/stevenvelozo/meadow",
  "devDependencies": {
    "chai": "4.3.6",
    "istanbul": "0.4.1",
    "mocha": "9.2.2",
    "alasql": "^0.4.0",
    "mysql2": "1.2.0"
  },
  "dependencies": {
    "async": "2.4.0",
    "fable": "~1.0.1",
    "foxhound": "~1.0.36",
    "is-my-json-valid": "2.16.0",
    "simple-get": "^4.0.1",
    "underscore": "^1.8.3"
  }
}
